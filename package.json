{
	"name": "share-code",
	"displayName": "Share Code Anytime",
	"description": "Share your code in anytime",
	"version": "0.3.0",
	"publisher": "liangzr",
	"engines": {
		"vscode": "^1.14.0"
	},
	"categories": [
		"Other"
	],
	"icon": "assets/logo.png",
	"activationEvents": [
		"onCommand:shareCode.openRemoteRepository",
		"onCommand:shareCode.shareWithPastebin"
	],
	"main": "./out/extension.js",
	"contributes": {
		"menus": {
			"editor/title": [
				{
					"command": "shareCode.openRemoteRepository",
					"title": "Share Code"
				}
			]
		},
		"commands": [
			{
				"command": "shareCode.openRemoteRepository",
				"title": "Share Code: Open Remote Repository"
			},
			{
				"command": "shareCode.shareWithPastebin",
				"title": "Share Code: Share with Pastebin (Public)"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.17.26",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^3.6.0",
		"@typescript-eslint/parser": "^3.6.0",
		"eslint": "^7.4.0",
		"eslint-config-airbnb-base": "^14.2.0",
		"eslint-plugin-import": "^2.22.0",
		"glob": "^7.1.6",
		"mocha": "^6.2.3",
		"tslint": "^5.20.1",
		"typescript": "^3.9.6",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"opn": "^6.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/liangzr/share-code-anytime"
	}
}
